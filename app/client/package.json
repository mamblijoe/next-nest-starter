{
    "name": "nextjs-starter",
    "version": "3.1.0",
    "private": true,
    "scripts": {
        "build": "next build",
        "dev": "next dev -p 5000",
        "prod": "next start",
        "type-check": "tsc --project tsconfig.json --noEmit --pretty",
        "lint": "next lint --format=pretty",
        "format": "pretty-quick --ignore-path .gitignore --pattern \"src/**/*.+(ts|js|tsx|json)\"",
        "analyze": "cross-env ANALYZE=true npm run build",
        "check-all": "run-p type-check lint format",
        "api-codegen": "openapi --input ./swagger.json --output ./src/api --request ./src/api/request.ts --useOptions",
        "storybook": "kill-port 6006 && storybook dev -p 6006 --quiet",
        "build-storybook": "storybook build",
        "build-tokens": "style-dictionary build --config tokens.config.json"
    },
    "dependencies": {
        "@sentry/nextjs": "7.47.0",
        "@tanstack/react-query": "4.29.1",
        "@tanstack/react-query-devtools": "4.29.1",
        "axios": "^1.3.5",
        "clsx": "^1.2.1",
        "dompurify": "3.0.1",
        "form-data": "4.0.0",
        "lodash": "4.17.21",
        "mobx": "^6.9.0",
        "mobx-react-lite": "3.4.3",
        "modern-normalize": "1.1.0",
        "nanoid": "^4.0.2",
        "next": "13.3.0",
        "next-seo": "6.0.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-query-kit": "1.4.1",
        "react-use": "17.4.0",
        "sharp": "0.32.0"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "13.3.0",
        "@storybook/addon-essentials": "7.0.3",
        "@storybook/addon-interactions": "7.0.3",
        "@storybook/addon-links": "7.0.3",
        "@storybook/addon-mdx-gfm": "7.0.3",
        "@storybook/nextjs": "7.0.3",
        "@storybook/react": "7.0.3",
        "@total-typescript/ts-reset": "0.4.2",
        "@types/dompurify": "3.0.1",
        "@types/lodash": "4.14.192",
        "@types/node": "^18.15.11",
        "@types/react": "18.0.35",
        "@types/react-dom": "^18.0.11",
        "@typescript-eslint/eslint-plugin": "^5.58.0",
        "@typescript-eslint/parser": "^5.58.0",
        "commitizen": "4.3.0",
        "cors": "2.8.5",
        "cross-env": "7.0.3",
        "css-loader": "6.7.3",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "^8.38.0",
        "eslint-config-next": "13.3.0",
        "eslint-config-webpractik": "github:webpractik/eslint-config-webpractik",
        "eslint-formatter-pretty": "5.0.0",
        "express": "4.18.2",
        "http-proxy-middleware": "2.0.6",
        "husky": "8.0.3",
        "kill-port": "^2.0.1",
        "lint-staged": "^13.2.1",
        "msw": "1.2.1",
        "msw-storybook-addon": "1.8.0",
        "npm-run-all": "4.1.5",
        "openapi-typescript-codegen": "0.24.0",
        "prettier": "^2.8.7",
        "pretty-quick": "3.1.3",
        "react-docgen-typescript": "2.2.2",
        "rimraf": "5.0.0",
        "sass": "^1.62.0",
        "sass-loader": "13.2.2",
        "storybook": "7.0.3",
        "storybook-dark-mode": "3.0.0",
        "style-dictionary": "3.7.2",
        "style-loader": "3.3.2",
        "typescript": "5.0.4"
    },
    "engines": {
        "node": ">= 16",
        "npm": ">= 8"
    },
    "browserslist": [
        "last 2 version",
        "> 1%",
        "not dead"
    ],
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "msw": {
        "workerDirectory": "public"
    }
}
